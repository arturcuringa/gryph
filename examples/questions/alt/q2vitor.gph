d : |(float,float), int| = 
    | (0.0,25.0) ? 0, (26.0, 50.0) ? 0, (51.0, 75.0) ? 0, (76.0, 100.0) ? 0 |; 

s : string;
n : float;  

while (n >= 0) {

    read s;

    n = s@float;
    
    for entry over d 
        if (n >= entry\0\\0\ and n <= entry\0\\1\) {
            d|entry\0\| = d|entry\0\| + 1;
            break;
        }
}

println d;
