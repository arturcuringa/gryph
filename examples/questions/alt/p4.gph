Rational_t {
	numerador : int;
	denominador : int;
}

sub make_rational(a, b : int) : Rational_t {
	rational : Rational_t = {numerador = a};
	if (b != 0)
		rational{denominador} = b;
	else
		rational{denominador} = 1;

	return rational;
}

sub equals(lhs, rhs : Rational_t) : bool {
	equals : bool = lhs{numerador}*rhs{denominador} ==
		lhs{denominador}*rhs{numerador};
	return equals;
}